{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/hello.js": {
      "runtime": "@vercel/node@3.2.8"
    },
    "api/spots.js": {
      "runtime": "@vercel/node@3.2.8"
    },
    "api/feedback.js": {
      "runtime": "@vercel/node@3.2.8"
    },
    "api/analyze.js": {
      "runtime": "@vercel/node@3.2.8"
    },
    "api/gemini-flash.js": {
      "runtime": "@vercel/node@3.2.8"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}